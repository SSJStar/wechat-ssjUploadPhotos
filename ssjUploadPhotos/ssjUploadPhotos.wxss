/* components/ssjUploadPhotos/ssjUploadPhotos.wxss */

.ssjUploadPhotosView {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  bottom: 100rpx;
  padding: 10rpx 30rpx 10rpx 30rpx;
  width: 100%;
  height: auto
}

.title {
  width: 100%;
  height: auto;
}

.childView {
  position: relative;
  width: 200rpx;
  height: 200rpx;
  margin: 10rpx 10rpx 10rpx 10rpx;
  margin-right: 10rpx;
  /* background-color: gainsboro; */
}

.childImage {
  position: absolute;
  z-index: 1;
  width: 180rpx;
  height: 180rpx;
}

.deleteIconImage {
  position: absolute;
  z-index: 5;
  margin-top: 0rpx;
  margin-left: 170rpx;
  width: 30rpx;
  height: 30rpx;
}
